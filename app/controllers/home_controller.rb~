class HomeController < ApplicationController

	def index
		@navbar="home"
		if user_signed_in?
			redirect_to :controller => 'dashboard', :action => 'index'
		end
	end
	
	def sevices
		@navbar= "services"
	end

	def news
		@navbar= "news"
	end

	def aboutUs
		@navbar= "aboutUs"
	end

	def contactUs
		@navbar= "contactUs"
	end

end
